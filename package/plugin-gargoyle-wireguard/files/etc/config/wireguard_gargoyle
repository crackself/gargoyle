config server 'server'
	option enabled '0'
	option ip '10.64.0.1'
	option submask '255.255.255.0'
	option port '51820'
	option c2c 'false'
	option lan_access 'true'
	option all_client_traffic 'true'

config client 'client'
	option enabled '0'
	option ip '10.64.0.2'
	option allow_nonwg_traffic 'true'

